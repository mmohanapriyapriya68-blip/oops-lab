#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> v;

    // Adding elements
    v.push_back(10);
    v.push_back(20);
    v.push_back(30);

    cout << "--- Vector Elements ---" << endl;
    for (int i = 0; i < v.size(); i++) {
        cout << v[i] << " ";
    }
    cout << endl;

    // Removing last element
    v.pop_back();
    cout << "--- After pop_back ---" << endl;
    for (int i : v) {
        cout << i << " ";
    }
    cout << endl;

    return 0;
}
