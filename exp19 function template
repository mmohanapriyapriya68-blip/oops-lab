#include <iostream>
using namespace std;

// Function template
template <class T>
T add(T a, T b) {
    return a + b;
}

template <class T>
T multiply(T a, T b) {
    return a * b;
}

int main() {
    // Integer type
    int x = 10, y = 20;
    cout << "--- Integer Operations ---" << endl;
    cout << "Addition       : " << add(x, y) << endl;
    cout << "Multiplication : " << multiply(x, y) << endl;

    // Float type
    float a = 5.5, b = 2.5;
    cout << "\n--- Float Operations ---" << endl;
    cout << "Addition       : " << add(a, b) << endl;
    cout << "Multiplication : " << multiply(a, b) << endl;

    return 0;
}
